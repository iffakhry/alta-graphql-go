# GraphQL schema example
#
# https://gqlgen.com/getting-started/

# type Todo {
#   id: ID!
#   text: String!
#   done: Boolean!
#   user: User!
# }

type User {
  id: Int
  name: String!
  email: String!
  password: String!
}

type Query {
  users: [User!]!
}

input NewUser {
  name: String!
  email: String!
  password: String!
}

type Mutation {
  createUser(input: NewUser!): User!
}
